<header>
    <%- include('./partials/header.ejs') %>
  </header>
  
 <% if (productos) { %>
    <div class="table-responsive" style="width: 100%; margin: 0;">
        <table class="table table-dark">
            <tr style="color: yellow;"> <th>Nombre</th> <th>Apellido</th> <th>Edad</th> </tr>
           <% for (const producto in productos) { %>
            <tr>
                <td> <%= productos[producto].title %> </td>
                <td>  <%= productos[producto].price %>  </td>
                <td> <img src=<%= productos[producto].thumbnail %> alt=""> </td>
            </tr>
            <% } %>
        </table>
    </div>
<% } else{ %>
   <h3 style="color: black">no existen datos</h3>
<% } %>

<div style="width: 100%;display: flex;margin: 20px">
        <button class="btn btn-secondary" style="width: 30%; margin: 0 auto; justify-content: center;align-items: center;" onclick="location.href='/'">Agregar productos</button>
</div>

